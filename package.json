  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github-personnel:F-Madruga/eslint-config.git"
  },
  "keywords": [],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@discordjs/opus": "0.8.0",
    "discord-player": "5.3.2",
    "discord.js": "14.6.0",
    "dotenv-safe": "8.2.0",
    "ffmpeg-static": "5.1.0"
  },
  "devDependencies": {
    "@types/dotenv-safe": "8.1.2",
    "@types/jest": "28.1.5",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "26.1.5",
    "jest": "28.1.3",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.20",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "ts-jest": "28.0.6",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "eslint": ">=7",
    "eslint-plugin-import": "^2.25.4"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "docker:compose:up": "docker-compose -f docker-compose.yml up --build",
    "docker:compose:down": "docker-compose -f docker-compose.yml down --rmi all",
    "deploy:commands": "tsc && node dist/tools/deploy-commands.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ]
}
